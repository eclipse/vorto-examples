{
  "name": "vorto-dashboard",
  "version": "0.5.0",
  "preferGlobal": true,
  "bin": {
    "vorto-dashboard": "./index.js"
  },
  "keywords": [
    "vorto",
    "eclipse",
    "eclipsevorto",
    "dashboard",
    "iot",
    "boschiotsuite"
  ],
  "license": "EPL-2.0",
  "author": "Tim Tenckhoff <tmtnckff@icloud.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse/vorto-examples.git"
  },
  "main": "./index.js",
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "argparse": "^1.0.10",
    "body-parser": "^1.19.0",
    "bootstrap": ">=3.4.1",
    "express": "^4.16.4",
    "jquery": "^3.4.1",
    "leaflet": "^1.4.0",
    "leaflet.awesome-markers": "^2.0.5",
    "loglevel": "^1.6.3",
    "medium-zoom": "^1.0.4",
    "popper.js": "^1.15.0",
    "react": "^16.2.0",
    "react-bootstrap": "0.32.1",
    "react-dom": "^16.2.0",
    "react-leaflet": "^2.2.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "4.2.2",
    "react-select": "^3.0.5",
    "react-syntax-highlighter": "10.2.1",
    "react-treeview": "^0.4.7",
    "redux": "^4.0.4",
    "redux-persist": "^5.10.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "reselect": "^4.0.0",
    "yarn": "^1.17.3"
  },
  "devDependencies": {
    "node-sass": "4.6.1",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "4.1.2",
    "react-scripts": "^3.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "dist": "mkdir dist && cp -r Simulators package_for_deployment.json things build README.md index.js dist/ && cd dist && mv package_for_deployment.json package.json && zip -r vorto-dashboard.zip .",
    "test": "react-scripts test --passWithNoTests",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
