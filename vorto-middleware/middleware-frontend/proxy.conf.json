{
  "/endpoint/*": {
    "target": "http://localhost:8080/",
    "secure": false,
    "ws": true,
    "logLevel": "debug"
  },
  "/api/*": {
    "target": "http://localhost:8080/",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true
  },
  "/login/*": {
    "target": "http://localhost:8080",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/mappings": ""
    }
  },
  
    "/login/oauth/*": {
      "target": "https://github.com/",
      "secure": false,
      "logLevel": "debug",
      "pathRewrite": {
      "^/mappings": ""
    },
      "changeOrigin": true
    }

  

}